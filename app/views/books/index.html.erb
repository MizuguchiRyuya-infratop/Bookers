<h2>New book</h2>

<%= form_for @book do |f| %>
  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title, class: 'book_title' %>
  </div>

  <div class="field">
    <%= f.label :body %>
    <%= f.text_area :body, class: 'book_body' %>
  </div>

  <div class="actions">
    <%= f.submit 'Create Book', data: { disable_with: "Creating..." } %>
  </div>
<% end %>